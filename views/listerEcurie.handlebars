<h1> Gestion des Ecuries </h1>
<a href="/ajouterEcurie/"><img src="/image/act_add.png"><p>Ajouter une ecurie</p></a>

<table>
      <tr>
        <th>Nom</th>
        <th>Directeur</th>
        <th>Points</th>
        <th>Actions</th>
      </tr>
      {{#each listeEcurie}}
        <tr>
            <td>{{nom}}</td>
            <td>{{dir}}</td>
            <td>{{point}} </td>
            <td><a href="/modifierEcurie/"><img src="/image/act_edit.png"><p>Modifier</p></a> <a href="/supprimerEcurie/"><img src="/image/act_delete.png"><p>Supprimer</p></a></td>
        </tr>
      {{/each}}
</table>
